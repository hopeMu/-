/* pages/uagr/uagr.wxss */
.page{}
.useragr{
  width: 100%;
  padding: 20rpx 50rpx;
  box-sizing: border-box;
}
.useragr-tit{
  width: 100%;
  height: 100rpx;
  line-height: 80rpx;
  text-align: center;
  border-bottom:4rpx solid #eee;
}

.useragr-fot{
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.fot-top{
  font-size: 26rpx;
}
.fot-top-color{
  color: #279B5F;
}
.fot-cen{
  margin-top: 20rpx;
  width: 70%;
  background-color: #279B5F;
  color: #fff;
  border-radius: 100rpx;

}
.fot-bot{
  margin-top: 20rpx;
}